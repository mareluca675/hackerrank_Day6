#include <bits/stdc++.h>

using namespace std;

int main()
{
    string s; 
    int i, j, n;
    
    cin >> n;
    cin.ignore();
    
    for(i = 1; i <= n; i ++)
    {
        getline(cin, s);
        
        for(j = 0; j < s.length(); j ++)
        {
            if(j % 2 == 0)
            {
                cout << s[j];
            }
        }
        
        cout << ' ';
        
        for(j = 0; j < s.length(); j ++)
        {
            if(j % 2 == 1)
            {
                cout << s[j];
            }
        }
        
        cout << '\n';
    }

    return 0;
}
